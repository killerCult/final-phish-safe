{
  "manifest_version": 2,
  "name": "Phish Safe",
  "description": "Check whether a website is a Phsihing Website",
  "version": "1.0",
  
  "icons": { "16": "img/evilphish.png",
           "48": "img/evilphish.png",
          "128": "img/evilphish.png" },

  "browser_action": {
    "default_icon": "img/evilphish.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
	"activeTab",
	"http://localhost/*",
	"storage"
  ]
}